<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.joonwook.realestate.Mapper.AptUdtMapper">
    <insert id="updateAptPrc" parameterType="com.joonwook.realestate.dto.AptPrcRegDto" >
        <![CDATA[
             UPDATE APT_PRC
                SET HIST_END_DTS = NOW(),
                    MOD_DTS = NOW()
             WHERE 1=1
             AND APT_DTL_ID = #{aptDtlId}
             AND PRC_TYPE_CD = #{prcTypeCd}
             AND DEAL_TYPE_CD = #{dealTypeCd}
        ]]>
    </insert>
</mapper>