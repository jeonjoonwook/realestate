<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.joonwook.realestate.Mapper.AptRegMapper">
    <insert id="insertApt" parameterType="com.joonwook.realestate.dto.AptRegDto" >
        <![CDATA[
            INSERT INTO APT (
                HIST_STRT_DTS,
                HIST_END_DTS,
                APT_NM,
                RGN_CD,
                RGN_DTL_CD,
                ADDRESS,
                APT_YEAR,
                UNIT_AMT,
                PLOT_RATE,
                REG_DTS,
                REGPE_ID,
                MOD_DTS,
                MODPE_ID
            ) VALUES (
                NOW(),
                '9999-12-31 23:59:59',
                #{aptNm},
                #{rgnCd},
                #{rgnDtlCd},
                #{address},
                #{aptYear},
                #{unitAmt},
                #{plotRate},
                NOW(),
                'joonwook',
                NOW(),
                'joonwook'
            )
        ]]>
    </insert>
</mapper>